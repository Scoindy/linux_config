alias bin="cd $HOME/bin"
alias tmp="cd $HOME/tmp"
alias dev="cd $HOME/dev"
alias log="cd $HOME/log"
alias tsh="cd $HOME/trash"
alias gbin="cd $GOPATH/bin"

alias h="history"
alias ls="ls -F"
alias sl="ls -F"
alias vi="vim"
alias dir="ls -l | grep ^d"
alias gpg="gpg2"
alias psme="ps -fu$(whoami)"
alias more="less"
alias zoom=". $HOME/.zoom"
alias trash="ls -latr $HOME_TSH | more"
alias empty="ls $HOME_TSH | xargs -i -t \rm -rf $HOME_TSH/{}; echo done."
alias mshare="sudo mount -t vboxsf _sprite /share"

alias rm="my_rm"
alias mv="mv -i"
alias cp="cp -i"

