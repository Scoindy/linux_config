export HOME_DEV="$HOME/dev"
export HOME_TMP="$HOME/tmp"
export HOME_LOG="$HOME/log"
export HOME_TSH="$HOME/trash"

export EDITOR="vi"
export HISTSIZE=10000
export HISTFILESIZE=20000
export PATH=".:$HOME/bin:$PATH"
export CLASSPATH="/usr/share/java"
export PS1="\[\033[1;33m\][\u@\h]\[\033[m\] \W \$ "

export LD_LIBRARY_PATH="/usr/include:/usr/lib/x86_64-linux-gnu:/usr/share/lintian/overrides"

set term=xterm-256color
set -o noclobber
set -o vi 
